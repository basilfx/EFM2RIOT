## Common CPU symbols
config CPU_FAM
    default "efr32mg14p" if CPU_FAM_EFR32MG14P

config CPU_MODEL
    default "efr32mg14p732f256im48" if CPU_MODEL_EFR32MG14P732F256IM48
    default "efr32mg14p731f256gm48" if CPU_MODEL_EFR32MG14P731F256GM48
    default "efr32mg14p732f256gm32" if CPU_MODEL_EFR32MG14P732F256GM32
    default "efr32mg14p733f256im48" if CPU_MODEL_EFR32MG14P733F256IM48
    default "efr32mg14p632f256gm48" if CPU_MODEL_EFR32MG14P632F256GM48
    default "efr32mg14p632f256gm32" if CPU_MODEL_EFR32MG14P632F256GM32
    default "efr32mg14p733f256gm48" if CPU_MODEL_EFR32MG14P733F256GM48
    default "efr32mg14p732f256gm48" if CPU_MODEL_EFR32MG14P732F256GM48
    default "efr32mg14p732f256im32" if CPU_MODEL_EFR32MG14P732F256IM32
