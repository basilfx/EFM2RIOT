config CPU_FAM_EFR32MG14P
    bool
    select CPU_CORE_CORTEX_M4F
    select CPU_COMMON_EFM32
    select HAS_PERIPH_HWRNG

## CPU Models
config CPU_MODEL_EFR32MG14P732F256IM48
    bool
    select CPU_FAM_EFR32MG14P

